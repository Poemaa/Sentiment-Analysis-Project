@model IEnumerable<Sentiment_Analysis_Project.Models.Analiza>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AnalizaId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rezultati)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Feedback)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.AnalizaId)
                </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rezultati)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Feedback.FeedbackId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.AnalizaId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.AnalizaId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.AnalizaId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
