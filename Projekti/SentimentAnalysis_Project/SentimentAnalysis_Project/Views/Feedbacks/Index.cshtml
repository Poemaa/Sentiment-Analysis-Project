@model IEnumerable<SentimentAnalysis_Project.Models.Feedback>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Permbajtja)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Data)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DegaFNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InstitutiFNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Permbajtja)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Data)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DegaFNavigation.Dega)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InstitutiFNavigation.Emri)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User.IdUser)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idfeedback">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idfeedback">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idfeedback">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
