@model IEnumerable<SentimentAnalysis_Project.Models.User>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Emri)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Mbiemri)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ditelindja)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Gjinia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FakultetiId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Statusi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FillimiStudimeve)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MesatarjaNotes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DegaNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InstitutiEmriNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Emri)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Mbiemri)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ditelindja)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Gjinia)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FakultetiId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Statusi)
