            <td>
                @Html.DisplayFor(modelItem => item.FillimiStudimeve)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MesatarjaNotes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DegaNavigation.Dega)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InstitutiEmriNavigation.Emri)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdUser">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdUser">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdUser">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
